{
  "manifest_version": 2,
  "name": "My Extension",
  "version": "1.0.0",
  "description": "A simple extension that does something",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
